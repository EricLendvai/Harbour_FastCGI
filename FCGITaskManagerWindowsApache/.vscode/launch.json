{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "harbour-dbg",
            "request": "launch",
            "name": "FCGITaskManager Down echo",
            "program": "${workspaceFolder}\\mingw64\\debug\\FCGITaskManager.exe",
            "arguments": ["down","http","localhost","8164","/fcgi_echo/","R:/Harbour_websites/fcgi_echo/","echo",""],
            "stopOnEntry": false,
            "preLaunchTask": "CompileDebug",
            "postDebugTask": "",
            "workingDir": "${workspaceFolder}\\",
            "sourcePaths": [
                "${workspaceFolder}\\"
            ]
        },
        {
            "type": "harbour-dbg",
            "request": "launch",
            "name": "FCGITaskManager Activate Echo",
            "program": "${workspaceFolder}\\mingw64\\debug\\FCGITaskManager.exe",
            "arguments": ["activate","http","localhost","8164","/fcgi_echo/","R:/Harbour_websites/fcgi_echo/","echo",""],
            "stopOnEntry": false,
            "preLaunchTask": "CompileDebug",
            "postDebugTask": "",
            "workingDir": "${workspaceFolder}\\",
            "sourcePaths": [
                "${workspaceFolder}\\"
            ]
        },
        {
            "type": "harbour-dbg",
            "request": "launch",
            "name": "FCGITaskManager Kill Echo",
            "program": "${workspaceFolder}\\mingw64\\debug\\FCGITaskManager.exe",
            "arguments": ["kill","http","localhost","8164","/fcgi_echo/","R:/Harbour_websites/fcgi_echo/","echo",""],
            "stopOnEntry": false,
            "preLaunchTask": "CompileDebug",
            "postDebugTask": "",
            "workingDir": "${workspaceFolder}\\",
            "sourcePaths": [
                "${workspaceFolder}\\"
            ]
        }
    ],
    "inputs": [
        {
            "id": "CCompiler",
            "type": "pickString",
            "description": "Select C Compiler",
            "options": ["mingw64","msvc64"],
            "default": "mingw64"
        }
    ]
}